@model IEnumerable<CashCard.Models.CutOff>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutBootstrap.cshtml";
}

<h2>Index</h2>

<p>
  
</p>

<table class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>
                Branch
            </th>
            <th>
                @Html.DisplayNameFor(model => model.State)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.PreviousBallance)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndBallance)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Note)
            </th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Branch.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.State)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.PreviousBallance)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EndBallance)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Note)
                </td>
                <td>

                    @Html.ActionLink("Details", "Details", new { id = item.Id })

                </td>
            </tr>
        }
    </tbody>
</table>

@section Scripts {
    @Scripts.Render("~/bundle/datatable")
    <script type="text/javascript">

      


    </script>
}




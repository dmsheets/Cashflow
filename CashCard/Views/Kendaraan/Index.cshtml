@model IEnumerable<CashCard.Models.Kendaraan>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutBootstrap.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NoKendaraan)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.JenisKendaraan)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EffStartDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EffEndDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MerkKendaraan)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ThnPembuatan)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NoKendaraan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.JenisKendaraan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EffStartDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EffEndDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MerkKendaraan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ThnPembuatan)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
